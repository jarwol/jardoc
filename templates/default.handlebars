<!DOCTYPE html>
<html lang="en">
<head>
    <link href="bootstrap.min.css" rel="stylesheet">
    <link href="default.css" rel="stylesheet">
</head>
<body>
<div class="row-fluid">
    <div class="span6 offset3">
        {{#key_val files}}
            <a href="javascript:toggleVisible('{{strip key}}');"><h2>{{key}}</h2></a><br/>

            <div id="{{strip key}}">
                {{#key_val val}}
                    <a href="javascript:toggleVisible('{{strip key}}');"><h3>{{key}}</h3></a><br/>
                    {{#key_val val}}
                        <a href="javascript:toggleVisible('{{strip key}}');"><h4>{{key}}</h4></a><br/>
                        <div id="{{strip key}}">
                            {{#key_val val}}
                                {{#unless val.private}}
                                    {{#eq ../../key "Functions"}}
                                        {{> function val}}
                                    {{/eq}}
                                    {{#eq ../../key "Fields"}}
                                        {{> field val}}
                                    {{/eq}}
                                    <hr/>
                                {{/unless}}
                            {{/key_val}}
                        </div>
                    {{/key_val}}
                {{/key_val}}
            </div>
        {{/key_val}}
    </div>
</div>
<script type="text/javascript">
    function toggleVisible(divId) {
        var div = document.getElementById(divId);
        if (div) {
            if (div.style.display === "none") {
                div.style.display = "block";
            }
            else {
                div.style.display = "none";
            }
        }
    }
</script>
</body>
</html>